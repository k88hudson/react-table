{
  "name": "react-tabular",
  "version": "0.0.1",
  "description": "",
  "main": "./dist/index.js",
  "scripts": {
    "build": "babel --presets es2015,react ./src --out-dir ./dist",
    "prebuild": "rm -rf dist && mkdir dist",
    "start": "npm run build && npm-run-all --parallel watch:* server",
    "prestart": "rm -rf www && mkdir www",
    "watch:dist": "npm run build -- --watch",
    "watch:js": "webpack -w -d",
    "watch:css": "node-sass ./demo -o ./www -w",
    "watch:static": "cpx \"src/static/**/*\" www",
    "server": "live-server --port=1989",
    "test": "karma start",
    "preversion": "npm run build",
    "postversion": "npm publish && git push origin master"
  },
  "author": "",
  "license": "MPL-2.0",
  "devDependencies": {
    "autoless": "^0.1.7",
    "babel-cli": "^6.1.2",
    "babel-core": "^6.1.2",
    "babel-loader": "^6.1.0",
    "babel-preset-es2015": "^6.1.2",
    "babel-preset-react": "^6.1.2",
    "cpx": "^1.2.1",
    "eslint": "^1.7.1",
    "faker": "^3.0.1",
    "ionicons": "^2.0.1",
    "karma": "^0.13.15",
    "karma-babel-preprocessor": "^5.2.2",
    "karma-chrome-launcher": "^0.2.1",
    "karma-firefox-launcher": "^0.1.6",
    "karma-mocha": "^0.2.1",
    "karma-mocha-reporter": "^1.1.1",
    "karma-sourcemap-loader": "^0.3.6",
    "karma-webpack": "^1.7.0",
    "live-server": "^0.8.1",
    "lodash": "^3.10.1",
    "mocha": "^2.3.4",
    "moment": "^2.10.6",
    "node-sass": "^3.4.1",
    "npm-run-all": "^1.2.12",
    "react": "^0.14.2",
    "react-autocomplete": "^0.1.4",
    "react-dom": "^0.14.0",
    "react-router": "^1.0.0-rc3",
    "react-select": "^0.9.1",
    "webpack": "^1.12.6",
    "webpack-notifier": "^1.2.1"
  },
  "dependencies": {
    "classnames": "^2.2.0",
    "lodash.clone": "^3.0.3"
  }
}
